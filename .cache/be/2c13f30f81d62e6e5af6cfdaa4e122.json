, error\n    referrer: \"no-referrer\", // no-referrer, *client\n    body: JSON.stringify(data), // body data type must match \"Content-Type\" header\n  })\n  .then(res => res.json())\n  .then(res => {\n    cfm.innerHTML = res.status;\n  })\n  .catch(error => console.error('Error:', error));\n}\n\n(function(){\n  visit();\n  document.getElementById(\"createFolderSubmit\").addEventListener(\"click\", function(e) {\n    e.preventDefault();\n    createFolder();\n  });\n})();"},"lineCount":null}},"hash":"f52e91f20bda138f74e4964bce0d4975","cacheData":{"env":{"ENDPOINT_PREFIX":"https://penelopeseul.netlify.com"}}}